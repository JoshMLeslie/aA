<h1>Session#new</h1>

<% flash[:errors].try(:each) do |error| %>
<%= error %><br>
<% end %>


<form action="<%= session_index_url %>"
      method="POST">
  <%= auth_token %>

  Email: <input type="text"
                name="user[email]"
                value="<%=@user.email%>">
  <br>
  Password: <input type="password"
                   name="user[password]">

  <br>
  <br>
  <input type="submit" value="Sign In!" >
</form>

<% content_for :footer do %>
<p>app/views/session/new.html.erb</p>
<% end %>
